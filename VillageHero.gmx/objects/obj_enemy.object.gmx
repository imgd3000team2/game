   }*/

    while(!place_meeting(x+sign(hsp),y,obj_hero))
    {
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;
 
//Vertical Collision
if (place_meeting(x,y+vsp,obj_wall))
{
    while(!place_meeting(x,y+sign(vsp),obj_wall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}

if (place_meeting(x,y+vsp,obj_hero))
{
    while(!place_meeting(x,y+sign(vsp),obj_hero))
    {
        y += sign(vsp);
    }
    vsp = 0;
}

y += vsp;
}

