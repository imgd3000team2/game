            <string>//see if hit
if(place_meeting(x, y, obj_sword_hitbox))
{
instance_destroy();
}

///React to inputs
//bleeding edge AI code

if(!instance_exists(obj_hero))
{
    instance_destroy();      
}
else
{
move = 0
if(abs(obj_hero.x - x) &gt; 5)
{
if(obj_hero.x &gt; x)
{
move = 1;
}
else
{
    if(x &gt; obj_hero.x)
    {        
        move = -1;
    }
}
}

hsp = move * movespeed;
if (vsp &lt; 20) vsp += grav;
 
if (place_meeting(x,y+1,obj_wall)|| place_meeting(x,y+1,obj_hero))
{
    vsp = jumping * -jumpspeed
}

if(jumping == 1)
{
    jumping = 0;
}

//Horizontal Collision
if (place_meeting(x+hsp,y,obj_wall))
{
    while(!place_meeting(x+sign(hsp),y,obj_wall))
    {
        x += sign(hsp);
    }
    hsp = 0;
    jumping = 1;
}


if (place_meeting(x+hsp,y,obj_hero))
{
    /*with (obj_hero)
   {
