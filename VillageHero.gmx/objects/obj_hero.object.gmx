            <string>///React to inputs, apply physics, detect collisions


///Key presses, set hero movement and facing direction
last_facing_direction = facing_direction;

//left
if(key_left == -1)
{
xdirection = x-32;
facing_direction = -1; 
}
else //right
{
if(key_right == 1)
{
xdirection = x+32;
facing_direction = 1;
}

//no change to facing direction.

}

//determine horizontal move
move = key_left + key_right;

//check jumping
hsp = move * movespeed;
if (vsp &lt; 20) vsp += grav;

if (place_meeting(x,y+1,obj_wall) || place_meeting(x,y+1,obj_hero))
{
vsp = key_jump * -jumpspeed
}

//Horizontal collisions with wall and other heroes
touchingwall = place_meeting(x+hsp,y,obj_wall);
touchinghero = place_meeting(x+hsp,y,obj_hero);
if (touchingwall || touchinghero)
{
if(touchingwall)
{
while(!place_meeting(x+sign(hsp),y,obj_wall))
{
x += sign(hsp);
}
}

if(touchinghero)
{  
while(!place_meeting(x+sign(hsp),y,obj_hero))
{
x += sign(hsp);
}
}

hsp = 0;
}

x += hsp;

//Vertical collsions with wall and other heroes
touchingwall = place_meeting(x,y+vsp,obj_wall);
touchinghero = place_meeting(x,y+vsp,obj_hero);

if (touchingwall|| touchinghero)
{
if(touchingwall){
while(!place_meeting(x,y+sign(vsp),obj_wall))
{
y += sign(vsp);
}
}

if(touchinghero){    
while(!place_meeting(x,y+sign(vsp),obj_hero))
{
y += sign(vsp);
}
}

vsp = 0;
}

y += vsp;

//collision with enemy
if(collision_rectangle( x-24, y+24, x+24, y-24, obj_enemy, true, true ))
{
instance_destroy();    

obj_control.heroes_alive--;

}

//deincrement sprite_wait_time
if(sprite_wait_time &gt; 0)
{
sprite_wait_time--;
}

///fighting
if(fight_wait_time &gt; 0)
{
fight_wait_time--;
}

if(key_fight &amp;&amp; (fight_wait_time == 0))
{
sprite_wait_time = 7;    //7 frames for attack

//left
if(facing_direction == -1)
{     
sprite_index = sprite_fight_left;
}
else 
{  //right
if(facing_direction == 1)
{        
sprite_index = sprite_fight_right;
}
}    

hitbox_inst_id = instance_create(xdirection, y, obj_sword_hitbox);

hitbox_inst_id.follow_id = id;

fight_wait_time = fight_countdown;
}
else
{
//If we are no longer fighting, change sprite if it is time to do so.
if((fight_wait_time == 0))
{
if(sprite_wait_time == 0)
{
//left
if(key_left == -1)
{   
if(sprite_index != sprite_move_left){   
sprite_index = sprite_move_left;
sprite_wait_time = 9;
}
}
else 
{   //right
if(key_right == 1)
{   
if(sprite_index != sprite_move_right)
{
sprite_index = sprite_move_right; 
sprite_wait_time = 9;       
}
}
else
{
if((facing_direction == -1) &amp;&amp;(sprite_index != sprite_standing_left))
{
sprite_index = sprite_standing_left;
}
else
{
if((facing_direction == 1)&amp;&amp;(sprite_index != sprite_standing_right))
{
sprite_index = sprite_standing_right;
} 
}
}
}   
}
}
}


